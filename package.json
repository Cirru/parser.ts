{
  "name": "@cirru/parser.ts",
  "version": "0.0.4",
  "description": "Cirru Parser in TypeScript",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack serve --hot --config webpack/dev.js",
    "watch": "webpack serve --hot --config webpack/dev.js",
    "release": "rm -rfv dist/* && time webpack --config webpack/release.js",
    "build": "yarn release",
    "serve": "http-server dist/ -s",
    "test": "yarn jest",
    "compile": "rm -rfv lib/* && tsc -d --project tsconfig-compile.json --outDir lib/",
    "prepare": "yarn test && yarn compile",
    "upload": "rsync -avr --progress dist/ cirru.org:~/repo/Cirru/parser.ts",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@emotion/css": "^11.1.3",
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/prettier": "^2.1.6",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "cache-loader": "^4.1.0",
    "css-loader": "^5.0.1",
    "dayjs": "^1.10.4",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^5.0.0",
    "html-webpack-tags-plugin": "^3.0.0",
    "immer": "^8.0.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.5",
    "prettier": "^2.2.1",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "thread-loader": "^3.0.1",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "typescript-styled-plugin": "^0.15.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.20.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {}
}
